{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch C++ Debugger",
      "type": "cppdbg",
      "request": "launch",
      "program": "/usr/bin/python3",
      "args": ["-u","examples/prowess_test.py"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}
